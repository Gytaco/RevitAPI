<Window x:Class="Application.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             Title="Wall Update" Width="300" Height="130">
    <Grid Height="164" VerticalAlignment="Top">
        <Button x:Name="bopenfile" Content="Open File" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="bopen_Clik"/>
        <Button x:Name="bgetResults" Content="Import Results" HorizontalAlignment="Left" Margin="197,71,0,0" VerticalAlignment="Top" Width="96" Click="getResults_Click"/>
        <TextBox x:Name="File_Location" HorizontalAlignment="Left" Height="23" Margin="13,30,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="270"/>

    </Grid>
</Window>
