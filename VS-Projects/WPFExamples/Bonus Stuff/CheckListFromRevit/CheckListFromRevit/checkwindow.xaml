<Window x:Class="CheckListFromRevit.checkwindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             Title ="Check List" Height="300" Width="300">
    <Grid>
        <ListBox x:Name="ObjectList" HorizontalAlignment="Left" Height="213" Margin="10,48,0,0" VerticalAlignment="Top" Width="198"/>
        <Button x:Name="bCreate" Content="CreateList" HorizontalAlignment="Left" Margin="213,241,0,0" VerticalAlignment="Top" Width="75" Click="bClick"/>
        <Button Content="Clear List" HorizontalAlignment="Left" Margin="213,216,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
    </Grid>
</Window>
